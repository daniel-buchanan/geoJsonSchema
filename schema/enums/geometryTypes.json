{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "id": "http://geojson.org/schema/geometry-types",
  "title": "GeoJSON Geometry Type",
  "description": "The valid values for the \"type\" property of GeoJSON geometry objects.",
  "link":"https://tools.ietf.org/html/rfc7946#section-1.4",
  "type": "string",
  "enum": [
    "Point",
    "LineString",
    "MultiPoint",
    "Polygon",
    "MultiLineString",
    "MultiPolygon",
    "GeometryCollection"
  ]
}
